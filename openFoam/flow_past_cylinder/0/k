FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.0021;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            zeroGradient;
    }
    cylinder
    {
        type            fixedValue; // kqRWallFunction;
        value           0.0021; // $internalField;
    }
    bndryWalls 
    {
        type            fixedValue; // kqWallFunction;
        value           0.0021; //$internalField; 
    }
}


